{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.0-beta3",
      "destination": "wwwroot/lib/bootstrap/"
    },    
    {
      "provider": "unpkg",
      "library": "fontawesome@5.6.3",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "popper.js@2.9.1",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "library": "fullPage.js@3.1.0",
      "destination": "wwwroot/lib/fullPage.js/"
    }
  ]
}